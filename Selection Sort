def selectionSort(arr):
    length=len(arr)
    for i in range (length-1):
        minIndex=i
        for j in range(i+1,length):
            if(arr[j]<arr[minIndex]):
                minIndex=j
        arr[i],arr[minIndex]=arr[minIndex],arr[i]
arr=[1,5,6,7,3,4,6,9]
selectionSort(arr)
print(arr)
        
